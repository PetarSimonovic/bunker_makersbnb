<h1>Welcome to Bunker!</h1>
<h3>Bunker listings</h3>
<br>

<ul>
  <% @properties.each do |property| %>
  <li><%=property.name%> </li>
  <ul>Property description: <%=property.description%></ul> 
  <ul>Price per night: <%=property.price%></ul>
    <form action="/booking/<%=property.property_id%>/<%=property.host_id%>" method="get">
      <input type="submit" value="Book property">
    </form>
  <% end %>
</ul>

